<template>
  <div id="app">
    <router-view></router-view>
  </div>
</template>

<script>
import storage from 'good-storage'
import { mapMutations } from 'vuex'
export default {
  name: 'app',
  created () {
    if (!window._dataInfo) {
      storage.get('_dataInfo') && (window._dataInfo = JSON.parse(storage.get('_dataInfo')))
    }
    this._getPost()
  },
  methods: {
    _getPost () {
      if (window._dataInfo) {
          this.$http.get('/jay/worker/postDict').then(res => {
          this.SET_POST(res.data.data.list)
        })
      }
    },
    ...mapMutations([
      'SET_POST'
    ])
  }
}
</script>
